@model Parkway.Report.ReportGeneratorModel;
@{
    ViewBag.Title = "Generate";


}

<h2>@Model.Title</h2>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <table>
        <tr>
            @foreach (var header in Model.Headers)
            {
                <th>@header</th>
            }
        </tr>
        @foreach (string rec in Model.RecordsInTable)
        {
            <tr>
                @{var records = rec.Split(','); }
                @foreach (var item in records)
                {
                    <td>item</td>
                }
            </tr>

        }

    </table>
</body>
</html>

